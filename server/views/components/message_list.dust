<div class="message-content">
<h2 class="room-header">{{ chat.currentRoom.name }}</h2>
</div>
<div class="message-list">
  <ul>
    <li class="message" ng-repeat="message in chat.messages">
      <p class="text">{{ message.message }}</p>
      <span class="author"><strong>author:</strong> {{ message.author }} ({{ message.time }})</span>
    </li>
  </ul>
</div>
